{
  "language": "en-US",
  "ignoreRegExpList": ["doxygen-commands", "example_tags"],
  "patterns": [
    {
      "name": "latex-command",
      "pattern": "/\\\\\\\\[a-z]+/g"
    },
    {
      "name": "javadoc-command",
      "pattern": "/@[a-z]+/g"
    },
    {
      "name": "doxygen-commands",
      "pattern": ["latex-command", "javadoc-command"]
    },
    {
      "name": "example_tags",
      "pattern": "/BUILD_[A-Z0-9_]+/g"
    }
  ],
